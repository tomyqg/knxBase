<html
	xmlns="http://www.w3.org/1999/xhtml"
	xmlns:wap="http://www.openwap.org/wap">
	<div id="edtDocumentRevision" class="wapForm">
		<h1 class="page-title">Edit Document Revision</h1>
		<wap:data>
			<fieldset>
				<legend>Editor: Document Revision</legend>
				<wap:dataform id="edtDocumentRevisionMain">
					<wap:rowdisplay data-wap-label="Document no." data-wap-attr="DocumentNo" />
					<wap:rowdisplay data-wap-label="Revision" data-wap-attr="Revision" />
					<wap:rowoption data-wap-label="Status" data-wap-attr="Status" data-wap-mode="edit"
						data-wap-db="Options" data-wap-key="Key" data-wap-value="Value" data-wap-cond="OptionName = 'DocumentStatus'" data-wap-order="Value" data-wap-def="" />
					<wap:rowinput data-wap-label="Filename" data-wap-attr="Filename" />
				</wap:dataform>
			</fieldset>
		</wap:data>
	</div>
</html>
