<html
	xmlns="http://www.w3.org/1999/xhtml"
	xmlns:dojo="http://www.openwap.org/dojo"
	xmlns:wap="http://www.openwap.org/wap">
<dojo:lbc style="width: 100%; height: 100%;">
	<wap:data>
		<table>
			<tr>
				<td valign="top">
					<fieldset>
						<legend>Editor: Customer Contact</legend>
						<wap:dataform id="formDiscountClassEdit">
							<wap:rowdisplay wapLabel="Supplier no." wapVar="SupplierNo" />
							<wap:rowinput wapLabel="Discount class wapVar="DiscountClass" />
							<wap:rowinput wapLabel="Quantity" wapVar="Quantity" />
							<wap:rowinput wapLabel="Discount" wapVar="Discount" />
							</wap:dataform>
					</fieldset>
				</td>
			</tr>
		</table>
		<wap:btnCreateDep form="formDiscountClassEdit" />
		<wap:btnUpdateDep form="formDiscountClassEdit" />
		<wap:btnCancelDep form="formDiscountClassEdit" />
	</wap:data>
</dojo:lbc>
</html>

