<html
	xmlns="http://www.w3.org/1999/xhtml"
	xmlns:dojo="http://www.openwap.org/dojo"
	xmlns:wap="http://www.openwap.org/wap">
<dojo:lbc style="width: 100%; height: 100%;">
	<wap:data>
		<table>
			<tr>
				<td valign="top">
					<fieldset>
						<legend>Lever Length Editor</legend>
						<wap:dataform id="formTroubleReportActionEdit">
							<wap:rowdisplay wapLabel="TroubleReport no." wapVar="TroubleReportNo" />
							<wap:rowinput wapLabel="Remark" wapVar="Remark" />
						</wap:dataform>
					</fieldset>
				</td>
			</tr>
		</table>
		<wap:btnCreateDep form="formTroubleReportActionEdit" />
		<wap:btnUpdateDep form="formTroubleReportActionEdit" />
		<wap:btnCancelDep form="formTroubleReportActionEdit" />
	</wap:data>
</dojo:lbc>
</html>
