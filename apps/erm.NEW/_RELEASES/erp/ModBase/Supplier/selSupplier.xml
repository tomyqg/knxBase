<html
	xmlns="http://www.w3.org/1999/xhtml"
	xmlns:wap="http://www.openwap.org/wap">
	<div>
		<h2 class="page-title">Select a Supplier</h2>
		<fieldset>
			<legend>Filter: Supplier</legend>
			<wap:dataform id="formSupplierSelectFilter">
				<wap:rowinput data-wap-label="Supplier no." data-wap-attr="SupplierNo" />
				<wap:rowinput data-wap-label="Name" data-wap-attr="Name" />
			</wap:dataform>
		</fieldset>
		<wap:grid data-wap-class="Supplier" data-wap-grid-name="gridSupplierSelectOV">
			<wap:gridcol data-wap-obj="Supplier" data-wap-attr="Id" data-wap-label="Id" />
			<wap:gridcol data-wap-obj="Supplier" data-wap-attr="SupplierNo" data-wap-label="SupplierNo" />
			<wap:gridcol data-wap-obj="Supplier" data-wap-attr="SupplierName" data-wap-label="Supplier Name" />
			<wap:gridcol data-wap-obj="Supplier" data-wap-attr="Name" data-wap-label="Name" />
		</wap:grid>
	</div>
</html>
