<html
	xmlns="http://www.w3.org/1999/xhtml"
	xmlns:dojo="http://www.openwap.org/dojo"
	xmlns:wap="http://www.openwap.org/wap">
<dojo:lbc style="width: 100%; height: 100%;">
	<wap:data>
		<table>
			<tr>
				<td valign="top">
					<fieldset>
						<legend>Editor: Article Group Item</legend>
						<wap:dataform id="formArticleGroupItemEdit">
							<wap:rowdisplay wapLabel="Item no." wapVar="ItemNo" />
							<wap:rowinput wapLabel="Component product group no" wapVar="CompProductGroupNo" />
							<wap:rowinput wapLabel="Component article group no" wapVar="CompArticleGroupNo" />
							<wap:rowinput wapLabel="Component article no" wapVar="CompArticleNo" />
							<wap:rowflag wapLabel="Generate" wapVar="Generate" wapMode="edit"
								db="Option" key="Key" value="Value" cond="OptionName = 'Flag'" order="ORDER BY Value" def="" />
							<wap:rowflag wapLabel="Catalog image" wapVar="CatImage" wapMode="edit"
								db="Option" key="Key" value="Value" cond="OptionName = 'Flag'" order="ORDER BY Value" def="" />
							</wap:dataform>
					</fieldset>
				</td>
			</tr>
		</table>
		<wap:btnCreateDep form="formArticleGroupItemEdit" />
		<wap:btnUpdateDep form="formArticleGroupItemEdit" />
		<wap:btnCancelDep form="formArticleGroupItemEdit" />
	</wap:data>
</dojo:lbc>
</html>