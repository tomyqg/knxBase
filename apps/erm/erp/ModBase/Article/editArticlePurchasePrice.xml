<html
        xmlns="http://www.w3.org/1999/xhtml"
        xmlns:wap="http://www.openwap.org/wap">
    <div>
        <wap:wapDialog data-wap-module="ModBase" data-wap-screen="ArticlePurchasePriceRel" data-wap-core-object="ArticlePurchasePriceRel" data-wap-core-object-key="Id" data-wap-parent-object="Article" w="300" h="300">
            <wap:key>
                <wap:keyform id="ArticlePurchasePriceRelKeyData">
                    <wap:keyrow data-wap-label="Id" data-wap-obj="ArticlePurchasePriceRel" data-wap-attr="Id" data-wap-prev="true" data-wap-next="true" />
                </wap:keyform>
            </wap:key>
            <wap:data>
                <wap:dataform id="formArticlePurchasePriceRel" class="formCreate formUpdate" w="500" h="400">
                    <table>
                        <wap:rowdisplay data-wap-label="Id" data-wap-attr="Id" />
                        <wap:rowdisplay data-wap-label="Article no." data-wap-attr="ArticleNo" />
                        <wap:rowinput data-wap-label="Supplier no." data-wap-attr="SupplierNo" />
                        <wap:rowinput data-wap-label="Supplier article no." data-wap-attr="SupplierArticleNo" />
                        <wap:rowinput data-wap-label="Old Supplier article no." data-wap-attr="SupplierArticleNoOld" />
                        <wap:rowinput data-wap-label="Supplier description" data-wap-attr="SupplierArticleDescription" />
                        <wap:rowinput data-wap-label="Quantity" data-wap-attr="Quantity" />
                        <wap:rowinput data-wap-label="Qty. per pack" data-wap-attr="QuantityPerPU" />
                        <wap:rowinput data-wap-label="Price" data-wap-attr="Price" />
                        <wap:rowoption data-wap-label="Currency" data-wap-attr="Currency"
                                       data-wap-db="Option"
                                       data-wap-key="Key" data-wap-value="Value"
                                       data-wap-cond="OptionName = 'Currency'"
                                       data-wap-order="`Key`" data-wap-def="default" />
                        <wap:rowinput data-wap-label="Supplier sales price" data-wap-attr="SupplierSalesPrice" />
                        <wap:rowinput data-wap-label="Own sales price" data-wap-attr="OwnSalesPrice" />
                        <wap:rowinput data-wap-label="Own discount factor" data-wap-attr="OwnDiscount" />
                        <wap:rowinput data-wap-label="Margin" data-wap-attr="Margin" />
                        <wap:rowinput data-wap-label="MKF" data-wap-attr="MKF" />
                        <wap:rowinput data-wap-label="HKRabKlasse" data-wap-attr="HKRabKlasse" />
                        <wap:rowinput data-wap-label="Calculation base" data-wap-attr="CalculationBase" />
                        <wap:rowinput data-wap-label="SPU Weight" data-wap-attr="SPUWeight" />
                        <wap:rowinput data-wap-label="SPUDimW" data-wap-attr="SPUDimW" />
                        <wap:rowinput data-wap-label="SPUDimH" data-wap-attr="SPUDimH" />
                        <wap:rowinput data-wap-label="SPUDimL" data-wap-attr="SPUDimL" />
                        <wap:rowinput data-wap-label="Valid from" data-wap-attr="ValidFrom" />
                        <wap:rowinput data-wap-label="Valid until" data-wap-attr="ValidUntil" />
                    </table>
                </wap:dataform>
            </wap:data>
        </wap:wapDialog>
    </div>
</html>
